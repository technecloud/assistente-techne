function ajustaNoticias() {
    var $divsNoticias = $('#main-news .row > div');
    if ($(window).width() < 786) {
        $($divsNoticias).find('article').css("min-height", 0);
    } else {
        if ($('#main-news .row > div').length > 0) {
            var $divsAlteradas = new Array();
            var widthNoticia = $divsNoticias.eq(2).width();
            var maxHeightNoticia = 0;
            for (var i = 0; i < $divsNoticias.length; i++) {
                if ($divsNoticias.eq(i).width() == widthNoticia) {
                    $divsAlteradas.push($divsNoticias.eq(i));
                    if ($divsNoticias.eq(i).height() > maxHeightNoticia) {
                        maxHeightNoticia = $divsNoticias.eq(i).height();
                    }
                }
            }
            for (var i = 0; i < $divsAlteradas.length; i++) {
                $($divsAlteradas[i]).find('article').css("min-height", maxHeightNoticia);
            }
        }
    }
}

var activitiesStep = 0;
var centerWidth = 0;
var activitiesLimitStep = 0;
var unitWidth = 0;

function scrollActivities() {
    var activities = $('.page-internacional #activities');
    activities.find('.arrow').remove();
    if ($(window).width() <= 1024) {
        activities.find('.scroll-wrapper').removeAttr('style');
        activities.find('.box-item').removeAttr('style');
    } else {
        var gridSpace = 15;
        var countUnits = (activities.find('.box-item').length + activities.find('.box-item.is-double').length);
        activitiesLimitStep = Math.floor(countUnits / 4);
        centerWidth = activities.find('.center').width() + 30;
        unitWidth = centerWidth / 4;
        activities.find('.scroll-wrapper').width(8000);
        activities.find('.box-item').css('flex-basis', unitWidth);
        activities.find('.box-item.is-double').css('flex-basis', unitWidth * 2);
        activities.append('<div class="arrow arrow-left"><i class="icon-left-open"></i></div>');
        activities.prepend('<div class="arrow arrow-right"><i class="icon-right-open"></i></div>');
        activities.find('.scroll-wrapper').css('margin-left', -gridSpace);
    }
}

function moveActivities(step) {
    if ($(window).width() <= 1024) {
        $('.page-internacional #activities').find('.scroll-wrapper').css('margin-left', 0);
    } else {
        var gridSpace = 15;
        if (activitiesStep < 0) activitiesStep = activitiesLimitStep;
        if (activitiesStep > activitiesLimitStep) activitiesStep = 0;
        var margin = ((unitWidth * 4) * activitiesStep * -1) - gridSpace;
        $('.page-internacional #activities').find('.scroll-wrapper').css('margin-left', margin);
    }
}
$('.page-internacional #activities').on('click', '.arrow-right', function() {
    moveActivities(activitiesStep++);
});
$('.page-internacional #activities').on('click', '.arrow-left', function() {
    moveActivities(activitiesStep--);
});
$(window).resize(function() {
    ajustaNoticias();
    scrollActivities();
});
$(window).scroll(function() {
    $('body').removeClass('scroll-down');
    if (scroll < $(window).scrollTop()) {
        $('body').addClass('scroll-down');
    }
    scroll = $(window).scrollTop();
    if ($(window).scrollTop() > 0) {
        $('body').addClass('header-fixed');
    } else {
        $('body').removeClass('header-fixed');
    }
    $('#main-banner .carousel-banner, .noticia-destaque .noticia-titulo .bloco-imagem, .noticia-imagem-cheia .bloco-imagem').each(function() {
        if ($(this).offset().top < $(window).scrollTop()) {
            var difference = $(window).scrollTop() - $(this).offset().top;
            var half = (difference / 2) + 'px',
                transform = 'translate3d( 0, ' + half + ',0)';
            $(this).find('img').css('transform', transform);
        } else {
            $(this).find('img').css('transform', 'translate3d(0,0,0)');
        }
    });
});
